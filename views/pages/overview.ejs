    <!-- Conteudo main da pagina em questão-->
    <div class="contentMainOverview">

        <!--Header da pagina em questão-->
        <%- include('../partials/header')  %> 
        <!--Header da pagina em questão-->
    
        <main>

            <div class="contentMainOverviewPadding">


                <div class="ContentMainOverviewTop">

                    <div class="ContentMainOverviewTopLeft">

                        <div class="boxsTopsMainOverview">
                            <div class="contentBoxsTopsMainOverview">
                                <p class="m-0">Unresolved</p>
                                <h2 class="numberContentBoxsTopsMainOverview m-0">60</h2>
                            </div>
                        </div>
    
                        <div class="boxsTopsMainOverview">
                            <div class="contentBoxsTopsMainOverview">
                                <p class="m-0">Overdue</p>
                                <h2 class="numberContentBoxsTopsMainOverview m-0">16</h2>
                            </div>
                        </div>

                    </div>

                    <div class="ContentMainOverviewTopRight">

                        <div class="boxsTopsMainOverview">
